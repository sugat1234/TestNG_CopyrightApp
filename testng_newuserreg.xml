<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="utilities.TestListener"></listener>
		<listener class-name="utilities.AnnotationTransformer"></listener>
	</listeners>

	<test thread-count="5" name="NewUserRegistrationSignUp-Chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.Test_NewUserRegistrationSignUp"></class>
		</classes>
	</test> <!-- NewUserRegistrationSignUp -->

	<test thread-count="5" name="NewUserRegistrationSignUp-Firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="tests.Test_NewUserRegistrationSignUp"></class>
		</classes>
	</test> <!-- NewUserRegistrationSignUp -->
</suite> <!-- Suite -->
